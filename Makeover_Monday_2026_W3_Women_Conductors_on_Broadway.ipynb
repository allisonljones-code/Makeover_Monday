    {
      "cell_type": "code",
      "source": [
        "# Convert to datetime (safe even if already datetime)\n",
        "date_cols = [\n",
        "    \"person_start_date\",\n",
        "    \"person_end_date\",\n",
        "    \"opening_date\",\n",
        "    \"closing_date\"\n",
        "]\n",
        "\n",
        "\n",
        "for c in date_cols:\n",
        "    df[c] = pd.to_datetime(df[c], errors=\"coerce\")\n",
        "\n",
        "END_FALLBACK = pd.Timestamp(\"2025-12-12\")\n",
        "\n",
        "start_dt = df[\"person_start_date\"].fillna(df[\"opening_date\"])\n",
        "\n",
        "end_dt = (\n",
        "    df[\"person_end_date\"]\n",
        "      .fillna(df[\"closing_date\"])\n",
        "      .fillna(END_FALLBACK)\n",
        ")\n",
        "\n",
        "##collapese the df to one row per paid (show_id, person_id)\n",
        "pairs = (\n",
        "    df.assign(start_dt=start_dt, end_dt=end_dt)\n",
        "      .groupby([\"show_id\", \"person_id\"], as_index=False)\n",
        "      .agg(\n",
        "          start_dt=(\"start_dt\", \"min\"),\n",
        "          end_dt=(\"end_dt\", \"max\")\n",
        "      )\n",
        ")\n",
        "\n",
        "df.head()"
      ],
