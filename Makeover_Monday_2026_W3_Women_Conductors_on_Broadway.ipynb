      "source": [
        "##build the data matrix : row = showid, personid pair, columns = year that the relationship is active - value = 0 or 1)\n",
        "years = np.arange(1915, 2026) #all the years from 1915 - 2025\n",
        "\n",
        "active = (\n",
        "    (years >= pairs[\"start_year\"].to_numpy()[:, None]) &\n",
        "    (years <= pairs[\"end_year\"].to_numpy()[:, None])\n",
        ").astype(int)\n",
        "\n",
        "df_years = pd.DataFrame(active, columns=years)\n",
        "\n",
        "df_data_base = pd.concat(\n",
        "    [pairs[[\"show_id\", \"person_id\"]], df_years],\n",
        "    axis=1\n",
        ")\n",
        "\n",
        "df_data_base.shape"
